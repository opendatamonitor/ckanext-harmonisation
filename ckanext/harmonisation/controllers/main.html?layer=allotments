                                                      <div dojoType="dijit.Tooltip" connectId="streetview" showDelay="0" label="Google Street View" position="below"></div>
													  <div dojoType="dijit.form.Button" id="clearmap" iconClass="clearmapIcon" onClick="makeIconActive(this); tb.deactivate(); navToolbar.deactivate(); map.graphics.clear(); turnidTool(0); closestreetview(); deselectPrintArea(); showmenu(); map.infoWindow.hide();"></div>
                                                      <div dojoType="dijit.Tooltip" connectId="clearmap" showDelay="0" label="Clear" position="below"></div>
												  </td>
											 </tr>
                </table>  
			
